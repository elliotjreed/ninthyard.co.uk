{
  "name": "elliotjreed/ninthyard.co.uk",
  "description": "",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Elliot J. Reed",
      "email": "ninthyard@elliotjreed.com",
      "homepage": "https://www.elliotjreed.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-pdo": "*",
    "phpmailer/phpmailer": "^6.1",
    "symfony/dotenv": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "ElliotJReed\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\ElliotJReed\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
